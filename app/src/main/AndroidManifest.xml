<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.SE.steamedboat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".addActivity" />
        <activity android:name=".Payment_solution"></activity>
        <activity android:name=".join" />
        <activity android:name=".Homepage" />
        <activity android:name=".create" />
        <activity android:name=".createORjoin" />
        <activity android:name=".Register" />
        <activity android:name=".ActivityDialog"
            android:theme="@style/Theme.AppCompat.Dialog.Alert"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MemberDialog"
            android:theme="@style/Theme.AppCompat.Dialog.Alert"></activity>
        <activity android:name=".SummaryPage"></activity>
        <activity android:name=".customise_split"></activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>